// Parameters
{
    "nb_elems": 100,
    "nb_gen": 5000,
    "pmut": 0.20, // Do nb_elems*pmut mutations
    "pcross": 0.40, // Do nb_elems*pcross/2 crossovers
    "evolutive": false, // true: re-evaluate identical elements
    "scaling": 2, // 0: None, 1: sigma_truncation, 2: ranking
    "normalize": 1, // 0: None, 1: f(x)=x/max, 2: f(x)=(x-min)/(max-min)
    "elitist": true, // true: Protect overall best element
    "sharing": 2, // 0: None, 1: Dynamic, 2: Dendrogram
    "dmax": 2.0, // Dynamic: max distance for same cluster, Dendrogram: max dissimilarity
    "pmax": 0.0, // Dendrogram: percentage of population as clusters
    "spenalty" : 1, // 0: None, 1: f/k, 2: f*(1-(k-1)/n)
    "sfactor": 0.99999, // 1.0: save only overall best, >1.0 : save none, <1.0 : save clusters best element
    "parallel": false, // true: evaluate with parallelism
    "verbose": 1, //0: nothing, 1:parameters, 2:best elem and clus stats, 3:debug
    "seed": 1234, // Random generator seed
    "ctrlc": true // If true, hitting Ctrl-c exists the AG main loop and returns results
}

