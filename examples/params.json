// Parameters
{
    "nb_elems": 200,
    "nb_gen": 2500,
    "pmut": 0.20,
    "pcross": 0.40,
    "evolutive": false,
    "scaling": 2, // 0: None, 1: sigma_truncation, 2: ranking
    "normalize": 1, // 0: None, 1: f(x)=x/max, 2: f(x)=(x-min)/(max-min)
    "elitist": true,
    "sharing": 2, // 0: None, 1: Dynamic, 2: Dendogram
    "dmax": 2.0, // Dynamic: max distance for same cluster, Dendogram: max dissimilarity
    "pmax": 1.0, // Dendogram: percentage of population as clusters
    "spenalty" : 1, // 0: None, 1: f/k, 2: f*(1-(k-1)/n)
    "sfactor": 0.99999, // 1.0: save only best, >1.0 : save none, <1.0 : save clusters best element
    "parallel": false,
    "verbose": 1, //0: nothing, 1:parameters, 2:best elem and clus stats, 3:debug
    "seed": 1234,
    "ctrlc": true
}

